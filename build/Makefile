# GNU Make solution makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug-simple
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug-simple)
  example_config = debug-simple
  test_config = debug-simple
endif
ifeq ($(config),debug-clever)
  example_config = debug-clever
  test_config = debug-clever
endif
ifeq ($(config),debug-clever-lazy)
  example_config = debug-clever-lazy
  test_config = debug-clever-lazy
endif
ifeq ($(config),debug)
  example_config = debug
  test_config = debug
endif
ifeq ($(config),release)
  example_config = release
  test_config = release
endif

PROJECTS := example test

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

example:
ifneq (,$(example_config))
	@echo "==== Building example ($(example_config)) ===="
	@${MAKE} --no-print-directory -C . -f example.make config=$(example_config)
endif

test:
ifneq (,$(test_config))
	@echo "==== Building test ($(test_config)) ===="
	@${MAKE} --no-print-directory -C . -f test.make config=$(test_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f example.make clean
	@${MAKE} --no-print-directory -C . -f test.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug-simple"
	@echo "  debug-clever"
	@echo "  debug-clever-lazy"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   example"
	@echo "   test"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
